{
	"name": "NamespaceData",
	"version": "2.0.0",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Wizardist Pavel Selitskas]",
		"[https://www.mediawiki.org/wiki/User:Karima_Rafes Karima Rafes]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:NamespaceData",
	"descriptionmsg": "nsdata-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.31.1"
	},
	"MessagesDirs": {
		"NamespaceData": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"NamespaceData": "NamespaceData.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "NamespaceData"
	},
	"Hooks": {
		"SkinTemplateNavigation::Universal": "NamespaceData::onSkinTemplateNavigation"
	},
	"ConfigRegistry": {
		"egNamespaceData": "NamespaceData::makeConfig"
	},
	"config_prefix": "egNamespaceData",
	"config": {
		"NamespaceData": {
			"description": "List of custom tabs",
			"value": {
				"data_main": {
					"namespace": 0,
					"target": 10000
				},
				"data_user": {
					"namespace": 2,
					"target": 10002
				}
			}
		}
	}
}
